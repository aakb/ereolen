/**
 * Implementation of hook_theme().
 */
function ebog_embed_youtube_theme() {
  return array(
    'ebog_embed_youtube' => array(
      'arguments' => array('conf' => NULL),
      'template' => 'ebog_embed_youtube',
    ),
  );
}

/**
 * Implementation of hook_ctools_plugin_directory().
 */
function ebog_embed_youtube_ctools_plugin_directory($module, $plugin) {
  if ($module == 'ctools' && !empty($plugin)) {
    return "plugins/$plugin";
  }
}
